{
    "type": "object",
    "required": [ "version", "curator" ],
    "properties": {
        "version": { "type": "integer" },
        "curator": { 
            "type": "object",
            "required": [ "name", "digitized" ],
            "properties": { 
                "name": {"type": "string"},
                "orcid": {"type": "string"},
                "digitized": {"type": "date"}
            }
        },
        "reviewers": [{ 
                "type": "object",
                "required": [ "name", "digitized" ],
                "properties": { 
                    "name": {"type": "string"},
                    "orcid": {"type": "string"},
                    "digitized": {"type": "date"}
                }
        }]
    }  
}  